{
    "name": "roommates",
    "version": "1.0.0",
    "description": "Suite of applications for roommate management.",
    "main": "index.js",
    "scripts": {
        "dev-build": "webpack --mode development --progress --watch --display-modules --config dashboard/webpack.dev.js",
        "dev-serve-api": "nodemon --watch index.js --watch api index.js",
        "dev": "concurrently --kill-others 'npm:dev-build' 'npm:dev-serve-api'",
        "build": "webpack --mode production --progress --config dashboard/webpack.prod.js",
        "start": "npm run build && NODE_ENV=production node index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fg123/roommates.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/fg123/roommates/issues"
    },
    "homepage": "https://github.com/fg123/roommates#readme",
    "dependencies": {
        "@babel/core": "^7.1.0",
        "@material/theme": "^0.39.1",
        "axios": "^0.18.0",
        "babel-loader": "^8.0.2",
        "body-parser": "^1.18.3",
        "chart.js": "^2.7.2",
        "chartjs-plugin-labels": "^1.1.0",
        "copy-webpack-plugin": "^4.5.2",
        "css-loader": "^1.0.0",
        "express": "^4.16.3",
        "express-mongo-db": "^2.0.4",
        "express-session": "^1.15.6",
        "google-auth-library": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "material-components-web": "^0.39.3",
        "mini-css-extract-plugin": "^0.4.3",
        "moment": "^2.22.2",
        "mongodb": "^3.1.6",
        "node-sass": "^4.9.3",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.0",
        "uuid": "^3.3.2",
        "vue": "^2.5.17",
        "vue-cli": "^2.9.6",
        "vue-loader": "^15.4.2",
        "vue-mdc-adapter": "^0.18.2",
        "vue-router": "^3.0.1",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.5.17",
        "webpack": "^4.19.1",
        "webpack-cli": "^3.1.0",
        "webpack-merge": "^4.1.4"
